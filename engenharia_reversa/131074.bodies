class Lists
!!!134402.php!!!	initialize()

		$this->setSource('tl_list');

		$this->hasManyToMany(
						'id',
						'ListItem',
						'id_list', 'id_item',
						'Item',
						'id'
		);

		$this->hasOne(
			'id_category',
			'Category',
			'id'
		);

		$this->belongsTo(
			'id_media',
			'Media',
			'id'
		);


